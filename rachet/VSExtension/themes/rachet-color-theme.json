{
  "name": "Rachet Standard",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6A9955",  // green for comments
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.control.rachet",
      "settings": {
        "foreground": "#569CD6"  // blue for keywords
      }
    },
    {
      "scope": "storage.type.rachet",
      "settings": {
        "foreground": "#4EC9B0"  // teal for types
      }
    },
    {
      "scope": "constant.numeric.rachet",
      "settings": {
        "foreground": "#B5CEA8"  // light green for numbers
      }
    },
    {
      "scope": "string.quoted.double.rachet",
      "settings": {
        "foreground": "#CE9178"  // orange for strings
      }
    },
    {
      "scope": "entity.name.function.rachet",
      "settings": {
        "foreground": "#DCDCAA"  // yellow for function names
      }
    },
    {
      "scope": "punctuation.separator.key-value.rachet",
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": "punctuation.terminator.rachet",
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": "keyword.operator.rachet",
      "settings": {
      "foreground": "#D16969"  // red for operators
      }
    }
  ]
}